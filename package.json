{
  "name": "relay-network-layer",
  "version": "2.2.0",
  "homepage": "https://github.com/4Catalyzer/relay-network-layer#readme",
  "bugs": {
    "url": "https://github.com/4Catalyzer/relay-network-layer/issues"
  },
  "license": "MIT",
  "author": "4Catalyzer",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/relay-network-layer.git"
  },
  "scripts": {
    "build": "4c build src",
    "format": "4c format .",
    "lint": "4c lint .",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "tdd": "jest --watch",
    "test": "yarn lint && yarn typecheck && yarn testonly --coverage",
    "testonly": "jest",
    "typecheck": "tsc --noEmit && tsc --noEmit -p test"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "yarn 4c lint --fix"
  },
  "prettier": "@4c/prettier-config",
  "jest": {
    "preset": "@4c/jest-preset",
    "moduleNameMapper": {
      "fetch-mock$": "<rootDir>/node_modules/fetch-mock/cjs/client.js"
    },
    "setupFiles": [
      "whatwg-fetch"
    ]
  },
  "release": {
    "conventionalCommits": true
  },
  "dependencies": {
    "@restart/hooks": "^0.3.26",
    "@types/node": "*",
    "@types/react": ">=16.0.0",
    "@types/relay-runtime": ">=10.1.8",
    "@types/store": "^2.0.2",
    "store": "^2.0.12",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@4c/babel-preset": "^8.0.3",
    "@4c/cli": "^2.2.1",
    "@4c/jest-preset": "^1.5.4",
    "@4c/prettier-config": "^1.1.0",
    "@4c/tsconfig": "^0.3.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "codecov": "^3.8.1",
    "eslint-config-4catalyzer-jest": "^2.0.10",
    "eslint-config-4catalyzer-react": "^1.0.13",
    "eslint-config-4catalyzer-typescript": "^3.0.3",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fetch-mock": "^9.11.0",
    "fetch-mock-jest": "^1.5.1",
    "hookem": "^2.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "relay-runtime": "^10.1.3",
    "socket.io-client": "^3.1.3",
    "socket.io-client-v2": "npm:socket.io-client@^2.4.0",
    "typescript": "^4.2.3",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0",
    "relay-runtime": ">=4.0.0",
    "socket.io-client": ">=2.0.4"
  },
  "peerDependenciesMeta": {
    "socket.io-client": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  }
}
